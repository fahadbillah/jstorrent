{
    "name": "JSTorrent",
    "description": "Javascript Torrent Client",
    "version": "0.3.4",
    "manifest_version": 2,
    "minimum_chrome_version": "24",
    "offline_enabled": true,
    "file_handlers": {
        "any": {
            "types": [
                "application/x-bittorrent"
            ],
            "extensions": [
                "torrent"
            ],
            "title": "Torrent downloader"
        }
    },
    "key": "MIGfMA0GCSqGSIb3DQEBAQUAA4GNADCBiQKBgQDCvpsh4qvVEOcUZPeucJJ5VASn8fIOGsoQIXnIewzRcqi3Nwj/4WttouI8Fp2OlNxjH6rkaFOSaUPv5n0j20M7clmTjFPmJtbdKKBdVnE5g1jRpkzwMPMV8fpP5IyyTy0hSkK1FAWuxnlBmOMLSAeqCsVH4cYO9s2ilFMNMEG04wIDAQAB",
    "permissions": [
        {"fileSystem":["write"]},
        {"socket":[
            "tcp-connect",
            "udp-bind::",
            "tcp-listen::",
            "udp-send-to::"
        ]},
        "storage",
        "experimental",
        "http://*/",
        "https://*/",
        "https://www.googleapis.com/"
    ],
    "oauth2": {
        "client_id": "432934632994-20rclui1m8od0p2g09vfbrdnk93gbraa.apps.googleusercontent.com",
        "scopes": ["https://www.googleapis.com/auth/drive.file",
                   "https://www.googleapis.com/auth/drive.install"
                  ]
    },
    "intents": {
        "http://webintents.org/view" : [{
            "title" : "BitTorrent File Viewing, Streaming and Downloading",
            "type" : ["application/x-bittorrent"]
        }],
        "http://webintents.org/share" : [{
            "title" : "BitTorrent File Sharing",
            "type" : ["image/*","video/*","*"]
        }]
    },
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "icons": { 
        "16": "icon16.png", 
        "48": "icon48.png", 
        "128": "icon128.png" 
    },
    "sandbox": {
        "pages": ["embedded_ga_host.html"]
    }
}
